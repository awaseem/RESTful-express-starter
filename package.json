{
  "name": "RESTful-express-starter",
  "version": "1.0.0",
  "description": "A simple RESTful API for a generic blog website",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/awaseem/blog-api.git"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-register --timeout 15000",
    "dev": "./node_modules/nodemon/bin/nodemon.js app.js --exec ./node_modules/babel-cli/bin/babel-node.js --presets es2015",
    "start": "node main.js"
