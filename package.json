    "babel-register": "^6.3.13",
    "body-parser": "^1.13.3",
    "express": "^4.13.3",
    "morgan": "^1.6.1",
    "nconf": "^0.8.2"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "mocha": "^2.3.4",
    "nodemon": "^1.8.1",
    "supertest": "^1.1.0"
  }
}
